import java.util.Scanner;                   //Двузначное число

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int z=sc.nextInt();
      int sum=z/10+z%10;
      
      boolean x=(sum>=10)&&(sum<100);
      boolean y=(sum>z);
      System.out.println(x);
      System.out.println(y);     
         }
}

