import java.util.Scanner;       //АВС

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int a=sc.nextInt();
      int b=sc.nextInt();
      int c=sc.nextInt();
      
      boolean f=(a<b)&&(b<c);
      boolean x=(b>a)&&(a>c);
      System.out.println(f);
      System.out.println(x);
      
      
      
  
  }
}

import java.util.Scanner;                     //Две задачи на квадрат и круг

public class Main {
    public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
     int a=sc.nextInt();
     int r=sc.nextInt();
     double pi=Math.PI;
    double s1=a*a;
    double s2=pi*r*r;
    boolean x=(s1>s2);
    System.out.println(s1);
    System.out.println(s2);  
    System.out.println(x);  
    double d=2*r;
    double c=Math.sqrt(2*a*a);
    if (d>c) {
      System.out.println("Квадрат впишется в круг");}
    else
    if (d<c){
    System.out.println("Круг впишется в квадрат");}
    else 
    if (d==c) {
      System.out.println("Круг впишется в квадрат, квадрат впишется в круг");
    }
    }
    }

